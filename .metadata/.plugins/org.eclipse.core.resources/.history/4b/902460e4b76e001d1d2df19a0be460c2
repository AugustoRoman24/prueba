	/*pedirCadena(&fecha[index].dia,"\nIngrese el dia: ");
	pedirCadena(&fecha[index].mes,"\nIngrese el mes: ");
	printf("\nIngrese el anio");
	fflush(stdin);
	scanf("%d",&fecha[index].anio);*/

	return retorno;
}
